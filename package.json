{
  "name": "contentful-next-js-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '*/**/*.{ts,tsx}' --fix --quiet",
    "format": "prettier --write ./src/**/*.{ts,tsx,scss}"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint '*/**/*.{ts,tsx}' --fix"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@contentful/rich-text-types": "^14.1.2",
    "classnames": "^2.3.1",
    "contentful": "^8.3.0",
    "contentful-management": "^7.15.2",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.32.11"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
